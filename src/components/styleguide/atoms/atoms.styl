atoms
  section
    margin-bottom 10px
    h3
      margin 0
      padding 0
      width 100vw
      height 30px
      border-bottom solid 1px #d1d1d1
      background-color #e8e8e8
      font normal 1em/30px "メイリオ",sans-serif
      span
        padding-left 15px
    > div
      padding-left 15px
      margin 10px 0
      &:empty
        height 40px
      > h5
        margin 5px 0
